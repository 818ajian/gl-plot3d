{
  "name": "gl-scene3d",
  "version": "1.0.0",
  "description": "Quick start rendering engine for plotting/data visualization",
  "main": "scene.js",
  "private": true,
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "gl-axes": "^6.0.2",
    "gl-mat4": "^1.1.2",
    "gl-select-static": "^1.0.2",
    "gl-spikes": "^2.0.1",
    "mouse-change": "^1.1.1",
    "ndarray": "^1.0.16",
    "orbiter": "^1.0.2"
  },
  "devDependencies": {
    "beefy": "^2.1.3",
    "browserify": "^9.0.3",
    "bunny": "^1.0.1",
    "canvas-fit": "^1.2.0",
    "dirichlet": "0.0.0",
    "gl-line-plot": "^2.3.1",
    "gl-scatter-plot": "^5.5.3",
    "gl-simplicial-complex": "^3.0.4",
    "gl-surface-plot": "^5.3.0",
    "ndarray": "^1.0.16",
    "ndarray-fill": "^1.0.1"
  },
  "scripts": {
    "scatter": "beefy ./example/scatter.js --open",
    "line": "beefy ./example/line.js --open",
    "surface": "beefy ./example/surface.js --open",
    "mesh": "beefy ./example/mesh.js --open"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gl-vis/gl-plot3d.git"
  },
  "browserify": {
    "transform": [
      "glslify"
    ]
  },
